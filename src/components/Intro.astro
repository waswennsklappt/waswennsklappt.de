---
export interface Props {
  title: string;
}
const { title } = Astro.props;
---

<header class="flex items-center content text-center justify-center mb-10">
  <h2 class="font-bold text-4xl px-5 mb-0">{title}</h2>
  <img width="120" height="120" src="/images/universe.png" alt="logo small" />
</header>
